			"events": {
				"contract_received_fund(address,uint256)": {
					"notice": "Emitted when funds are received into the wallet"
				},
				"contract_transfered_fund(address,uint256)": {
					"notice": "Emitted when funds are transferred out of the wallet"
				}
			},
			"kind": "user",
			"methods": {
				"accessControl()": {
